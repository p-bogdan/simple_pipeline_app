pipeline {
    agent any
     environment {    
       gcp_project_id = 'lab-project-282605'  
     }
    stages {
       stage('Obtain K8s credentials') {
            steps {
               echo 'Obtaining k8s credentials'
               sh 'gcloud container clusters get-credentials k8s-lab --zone europe-west1-b --project ${env.gcp_project_id}'
            }
       }
    }
}